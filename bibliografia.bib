@InProceedings{biswas2012hamiltonian,
author="Biswas, Sudip
and Durocher, Stephane
and Mondal, Debajyoti
and Nishat, Rahnuma Islam",
title="Hamiltonian Paths and Cycles in Planar Graphs",
booktitle="Combinatorial Optimization and Applications",
year="2012",
publisher="Springer",
pages="83--94",
abstract="We examine the problem of counting the number of Hamiltonian paths and Hamiltonian cycles in outerplanar graphs and planar graphs, respectively. We give an O(n$\alpha$n) upper bound and an $\Omega$($\alpha$n) lower bound on the maximum number of Hamiltonian paths in an outerplanar graph with n vertices, where $\alpha${\thinspace}≈{\thinspace}1.46557 is the unique real root of $\alpha$3{\thinspace}={\thinspace}$\alpha$2{\thinspace}+{\thinspace}1. For any positive integer n{\thinspace}≥{\thinspace}6, we define an outerplanar graph G, called a ZigZag outerplanar graph, such that the number of Hamiltonian paths starting at a single vertex in G is the maximum over all possible outerplanar graphs with n vertices. Finally, we prove a 2.2134nupper bound on the number of Hamiltonian cycles in planar graphs, which improves the previously best known upper bound 2.3404n.",
isbn="978-3-642-31770-5"
}


